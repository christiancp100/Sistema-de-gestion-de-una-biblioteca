<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="fondo" prefHeight="244.0" prefWidth="272.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@css/VPrincipal.css" />
   </stylesheets>
   <center>
      <Label id="textoAviso" fx:id="mensajeTxt" alignment="CENTER" text="Texto de Aviso" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="30.0" top="60.0" />
         </BorderPane.margin>
      </Label>
   </center>
   <bottom>
      <Button fx:id="boton" mnemonicParsing="false" onAction="#cerrarVentana" text="Aceptar" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="80.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
</BorderPane>
